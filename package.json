{
  "name": "@appscode/vue-ssr",
  "version": "0.0.1",
  "description": "A vue component to preserve server side rendered html",
  "main": "dist/vue-ssr.ssr.js",
  "browser": "dist/vue-ssr.esm.js",
  "module": "dist/vue-ssr.esm.js",
  "unpkg": "dist/vue-ssr.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "!src/serve-dev.*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve src/serve-dev.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appscode/vue-ssr.git"
  },
  "keywords": [
    "vue",
    "nuxt",
    "ssr",
    "appscode"
  ],
  "author": "AppsCode Inc.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/appscode/vue-ssr/issues"
  },
  "homepage": "https://github.com/appscode/vue-ssr#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-replace": "^2.2.1",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "minimist": "^1.2.0",
    "prettier": "^2.0.2",
    "rollup": "^1.27.13",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "engines": {
    "node": ">=8"
  }
}
